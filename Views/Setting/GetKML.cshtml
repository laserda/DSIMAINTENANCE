@model DSIAppMaintenance.Models.CodeForm
@{
    ViewData["Title"] = "KML";
}

<h2>Build Report code</h2>
<div class="row">
    <form asp-controller="Setting" asp-action="GetKML" asp-anti-forgery="true" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <div class="form-group">
                <input asp-for="textGetFile" class="form-control-file" placeholder="Entrez le nom du fichier" type="file" multiple />
            </div>
            <div class="form-group">
                <input asp-for="txtNameReport" class="form-control" placeholder="Entrez le nom de l'état" type="text" />
            </div>
            <div class="form-group">
                <input asp-for="txtNameTableAd"  class="form-control" placeholder="Entrez le nom de l'état" type="text" />
            </div>
            <button type="submit" class="btn btn-primary">Générer le code</button>

        </div>
    </form>
</div>

@section Scripts {
    @Html.Partial("_ValidationScriptsPartial")
}


