@model DSIAppMaintenance.Models.CodeForm
@{
    ViewData["Title"] = "Build Report code automatique";
}

<h2>Build Report code</h2>
<div class="row">
    <form asp-controller="Setting" asp-action="GenCodeEtatAuto" asp-anti-forgery="true" method="post" enctype="multipart/form-data">
        <div class="form-group">  
            <div class="form-group">
                <input asp-for="textGetFile" class="form-control-file" placeholder="Entrez le nom du fichier" type="file" multiple />
            </div>
            <div class="form-group">
                <input asp-for="txtNameReport" class="form-control" value="rptv_et_hand" placeholder="Entrez le nom de l'état" type="text" />
            </div>
            <div class="form-group">
                <input asp-for="txtNameTableAd" value="datasetWdocumplant" class="form-control" placeholder="Entrez le nom de l'état" type="text" />
            </div>
           <button type="submit" class="btn btn-primary">Générer le code</button>

        </div>
    </form>
</div>

@section Scripts {
    @Html.Partial("_ValidationScriptsPartial")
}


